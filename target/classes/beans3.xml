<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" 
       xmlns:context="http://www.springframework.org/schema/context" 
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xsi:schemaLocation=" http://www.springframework.org/schema/beans  
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
                           http://www.springframework.org/schema/context  
                           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
 <context:annotation-config/>
<bean id="humanheart" class="com.bridgeit.autowired.Heart">
<property name="nameOfAnimal" value="Human"/>
<property name="noOfHear" value="1"/>
</bean>


 <bean id="octpousHeart" class="com.bridgeit.autowired.Heart">
 <property name="nameOfAnimal" value="Human"/>
<property name="noOfHear" value="3"/>
 </bean>
 <bean id="human" class="com.bridgeit.autowired.Human" >
 
 
 </bean>
 </beans>